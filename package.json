{
  "name": "queueable",
  "version": "4.1.1",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/queueable.js",
    "default": "./dist/queueable.modern.mjs",
    "types": "./dist/index.d.ts"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/queueable.js",
  "module": "./dist/queueable.module.js",
  "unpkg": "./dist/queueable.umd.js",
  "description": "Convert push-based streams to pull-based async iterables",
  "keywords": [
    "async",
    "queue",
    "es2018",
    "iteration",
    "async-iterators",
    "esnext",
    "streams"
  ],
  "files": [
    "dist"
  ],
  "author": "slikts <dabas@untu.ms>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slikts/queueable.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "prebuild": "rimraf dist",
    "build": "microbundle --external none",
    "dev": "microbundle watch",
    "test": "jest",
    "test:watch": "jest --watch --runInBand --verbose false",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"yarn commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/eslint": "^8",
    "@types/fast-list": "^1.0.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^16.4.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-jest": "^29.3.1",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.2",
    "cross-env": "^7.0.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^8.32.0",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^6.0.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.0",
    "microbundle": "^0.15.1",
    "prettier": "^2.0.1",
    "rimraf": "^3.0.0",
    "ts-node": "^10.1.0",
    "typedoc": "^0.20.35",
    "typescript": "^4.9.4",
    "validate-commit-msg": "^2.12.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "fast-list": "^1.0.3"
  },
  "packageManager": "yarn@3.3.1"
}
