{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "types": ["node", "jest"],
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "include": ["src/**/*"]
}
